{
  "api": {
    "base_url": "http://httpbin.org/post",
    "timeout": 30,
    "sleep_between_requests": 0.1,
    "max_retries": 1
  },
  "excel": {
    "input_file": "Card Recommendation avg gmv dump.xlsx",
    "output_file": "test_output.xlsx",
    "sheet_name": 0
  },
  "column_mappings": {
    "user_id": "userid",
    "amazon_spends": " avg_amazon_gmv ",
    "flipkart_spends": " avg_flipkart_gmv ",
    "myntra": " avg_myntra_gmv ",
    "ajio": " avg_ajio_gmv ",
    "avg_gmv": " avg_confirmed_gmv ",
    "grocery": " avg_grocery_gmv ",
    "total_gmv": " total_gmv "
  },
  "processing": {
    "top_n_cards": 3,
    "extract_spend_keys": [
      "amazon_spends",
      "flipkart_spends",
      "grocery_spends_online",
      "other_online_spends"
    ],
    "skip_empty_rows": true,
    "continue_on_error": true,
    "other_online_mode": "sum_components"
  }
}